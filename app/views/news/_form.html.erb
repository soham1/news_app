<%= form_for @news do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :summary %><br>
    <%= f.text_area :summary %>
  </p>

  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.label :date %><br>
    <%= f.date_field :date %>
  </p>

  <p>
    <%= f.check_box :online %>
    <%= f.label :online %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>